<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/dashboard_navigation.xml"
    app:startDestination="@id/home">

    <fragment
        android:id="@+id/home"
        android:name="com.hamid.template.ui.dashboard.fragments.HomeFragment"
        android:label="home"
        tools:layout="@layout/fragment_home">

    </fragment>

    <fragment
        android:id="@+id/Maps"
        android:name="com.hamid.template.ui.dashboard.fragments.MapFragment"
        android:label="MapFragment"
        tools:layout="@layout/fragment_map">
    </fragment>
    <fragment
        android:id="@+id/Settings"
        android:name="com.hamid.template.ui.dashboard.fragments.SettingsFragment"
        android:label="SettingsFragment"
        tools:layout="@layout/fragment_settings">
    </fragment>
    <fragment
        android:id="@+id/help"
        android:name="com.hamid.template.ui.dashboard.fragments.HelpFragment"
        android:label="HelpFragment"
        tools:layout="@layout/fragment_help">
    </fragment>
    <fragment
        android:id="@+id/SelectFacility"
        android:name="com.hamid.template.ui.dashboard.fragments.FacilitiesForTransportation"
        android:label="SelectFacility"
        tools:layout="@layout/facilities_fragment">
    </fragment>
    <action
        android:id="@+id/action_home_to_Settings"
        app:destination="@id/Settings" />
    <action
        android:id="@+id/action_home_to_Maps"
        app:destination="@id/Maps" />
    <action
        android:id="@+id/action_home_to_help"
        app:destination="@id/help" />
    <action
        android:id="@+id/action_home_to_facilities"
        app:destination="@id/SelectFacility" />
</navigation>